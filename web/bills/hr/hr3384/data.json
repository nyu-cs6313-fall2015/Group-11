  "introduced_at": "2013-10-30", 
  "number": "3384", 
  "official_title": "To amend title 38, United States Code, to ensure that veterans may attend pre-apprenticeship programs using certain educational assistance provided by the Secretary of Veterans Affairs, and for other purposes.", 
  "popular_title": null, 
  "related_bills": [], 
  "short_title": "Veterans' Entry to Apprenticeship Act", 
  "sponsor": {
    "district": "11", 
    "name": "Bentivolio, Kerry L.", 
    "state": "MI", 
    "thomas_id": "02135", 
